<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation_cust"
    app:startDestination="@+id/nav_home_cust_frag">

    <fragment
        android:id="@+id/nav_home_cust_frag"
        android:name="com.darthwithap.homeapp.HomeFragmentCust"
        android:label="HomeCust"
        tools:layout="@layout/fragment_home_cust" />


    <fragment
        android:id="@+id/nav_settings_cust_frag"
        android:name="com.darthwithap.homeapp.SettingsFragmentCust"
        android:label="SettingsCust"
        tools:layout="@layout/fragment_settings_cust">

        <action
            android:id="@+id/about_dest"
            app:destination="@+id/nav_about_cust_frag"
            />

    </fragment>

    <fragment
        android:id="@+id/nav_about_cust_frag"
        android:name="com.darthwithap.homeapp.AboutFragmentCust"
        android:label="AboutCust"
        tools:layout="@layout/fragment_about_cust" />

    <fragment
        android:id="@+id/nav_reviews_cust_frag"
        android:name="com.darthwithap.homeapp.ReviewsFragmentCust"
        android:label="ReviewsCust"
        tools:layout="@layout/fragment_reviews_cust" />

    <fragment
        android:id="@+id/nav_bookings_cust_frag"
        android:name="com.darthwithap.homeapp.BookingsFragmentCust"
        android:label="BookingsCust"
        tools:layout="@layout/fragment_bookings_cust" />
</navigation>